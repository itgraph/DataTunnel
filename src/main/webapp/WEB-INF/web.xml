<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>DataTunnelServlet</servlet-name>
        <servlet-class>org.itgraph.core.DataTunnelServlet</servlet-class>
        <init-param>
            <param-name>driverClass</param-name>
            <param-value>com.mysql.cj.jdbc.Driver</param-value>
        </init-param>
         <init-param>
            <param-name>url</param-name>
            <param-value>jdbc:mysql://127.0.0.1:3306/demo?useUnicode=true&amp;characterEncoding=UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>username</param-name>
            <param-value>root</param-value>
        </init-param>
        <init-param>
            <param-name>password</param-name>
            <param-value>123456</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataTunnelServlet</servlet-name>
        <url-pattern>/dataTunnel</url-pattern>
    </servlet-mapping>
</web-app>